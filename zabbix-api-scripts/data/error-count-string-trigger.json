{
    "jsonrpc": "2.0",
    "method": "trigger.create",
    "params": 
        {
            "description": "Lambda Function keeps failing",
            "expression": "countunique(/zblamb-lambda-errors/error-count-string,5m,\"regexp\",\"([A-Za-z0-9]+),\\1,\\1\")>0",
            "priority": 4,
            "manual_close": 1,
            "type": 1,
            "correlation_mode": 1,
            "correlation_tag": "Lambda Name",
            "tags": [
                {
                    "tag": "Lambda Name",
                    "value": "{{ITEM.VALUE}.regsub(\"([A-Za-z0-9]+),\\1,\\1\", \"\\1\")}"
                }
            ]
        },
    "auth": "ZBLAMB_TOKEN",
    "id": 1
}